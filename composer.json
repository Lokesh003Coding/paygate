{
    "name": "pwparsons/paygate",
    "description": ":description",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Parsons",
            "email": "peterw.parsons@gmail.com"
        }
    ],
    "homepage": "https://github.com/pwparsons/paygate",
    "keywords": ["Laravel", "PayGate"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PWParsons\\PayGate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PWParsons\\PayGate\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PWParsons\\PayGate\\PayGateServiceProvider"
            ],
            "aliases": {
                "PayGate": "PWParsons\\PayGate\\Facades\\PayGate"
            }
        }
    }
}
